---
import { Image } from 'astro:assets';
import logo_syntex_greentech from '~/assets/images/logo/logo_syntex_greentech.svg';
import logo_syntex_greentech_dark from '~/assets/images/logo/logo_syntex_greentech_dark.svg';
---

<>
  <Image src={logo_syntex_greentech} alt="qr code vcard" width={120} height={120} format="webp" class="dark:hidden" />
  <Image
    src={logo_syntex_greentech_dark}
    alt="qr code vcard"
    width={120}
    height={120}
    format="webp"
    class="hidden dark:block"
  />
</>
